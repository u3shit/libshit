#define inline constexpr inline
#pragma push_macro("inline")
#define inline inline _Pragma("pop_macro(\"inline\")")
#pragma push_macro("inline")
#define inline inline _Pragma("pop_macro(\"inline\")")

#include_next<xutility>
#pragma pop_macro("inline")
